<% if company.errors.any? %>
  <p>Foram encontrados <%= company.errors.count %> erro(s):</p>
  <ul>
    <% company.errors.full_messages.each do |error|%>
      <li><%= error %></li>
    <% end %>
  </ul>
<% end %>
<%= form_with model: company do |f| %>
  <%= f.label :corporate_name, 'Razão social'%>
  <%= f.text_field :corporate_name %>
  <%= f.label :address, 'Endereço' %>
  <%= f.text_field :address %>
  <%= f.label :email, 'Email'%>
  <%= f.text_field :email %>
  <%= f.label :cnpj, 'CNPJ'%>
  <%= f.text_field :cnpj %>
  <%= f.submit button_text %>
<% end %>